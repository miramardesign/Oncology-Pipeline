{
  "name": "serono-pipeline",
  "version": "1.0.0",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "deploy": "ng build --prod --base-href \"https://miramardesign.github.io/Oncology-Pipeline/serono-pipeline/\"  && ngh --dir dist/serono-pipeline", 
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && electron .",
    "build-windows": "ng build --base-href ./  --prod && electron-packager . --overwrite --asar --ignore=README.md --platform=win32 --out=electron-builds",
    "build-mac": "ng build --base-href ./  --prod && electron-packager . --overwrite --asar --ignore=README.md --platform=darwin --out=electron-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngtools/webpack": "^7.3.3",
    "angular-cli-ghpages": "^0.6.0",
    "angular-user-idle": "^2.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "ng-bootstrap": "^1.6.3",
    "ngx-bootstrap": "^3.2.0",
    "ngx-swiper-wrapper": "^7.2.1",
    "rxjs": "6.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.3",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "asar": "^1.0.0",
    "codelyzer": "~4.2.1",
    "electron": "^4.0.8",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.8"
  }
}
